{
"update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ImproveTagList for Qiita",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Enable to click tag list and show entries filtered by user and tag",
  "author": "Toshinari Suzuki",
  "homepage_url": "http://suzuki.toshinari.jp",
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "content_scripts": [{
    "matches": ["*://qiita.com/*"],
    "run_at": "document_end",
    "js": ["script.js"],
    "all_frames": true
  }]
}
